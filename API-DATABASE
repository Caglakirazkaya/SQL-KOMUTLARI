create table Users(
 RowId int,
 UserId uniqueidentifier,
 Name nvarchar(100),     
 Surname nvarchar(100),     
 UserName nvarchar(100),     
 Country nvarchar(100),     
 City nvarchar(100),     
 PhoneNumber nvarchar(100),     
 PhoneCompleted bit,     
 Email nvarchar(100),     
 EmailCompleted bit,     
 CreateDate datetime,     
 IsActive int
 )
